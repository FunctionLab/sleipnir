package	"Combiner"
version	"1.0"
purpose	"PCL and data file combination tool"

section "Optional"
option	"type"		t	"Data file type"	values="pcl","dat","dab"	default="pcl"
option	"method"	m	"Combination method"	values="max","mean"	default="mean"
option	"output"	o	"Output file"	string	typestr="filename"
option	"subset"	s	"Subset size (none if zero)"	int	default="0"
option	"verbosity"	v	"Message verbosity"	int	default="5"
