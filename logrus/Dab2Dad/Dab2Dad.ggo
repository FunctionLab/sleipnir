package	"Dab2Dad"
version	"1.0"
purpose	"Single/multiple data file interconversion"

defgroup "input-output"
groupoption	"input"		i	"Input dad file"	string	typestr="filename"	group="input-output"
groupoption	"output"	o	"Output dad file"	string	typestr="filename"	group="input-output"
groupoption	"load"		l	"Persistent load dad file"	string	typestr="filename"	group="input-output"
section "Optional"
option	"verbosity"	v	"Message verbosity"	int	default="5"
