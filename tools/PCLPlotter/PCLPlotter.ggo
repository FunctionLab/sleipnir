package	"PCLPlotter"
version	"1.0"
purpose	"Plots summary information for PCL files/clusters."

section "Main"
option	"input"		i	"Input PCL file"
						string	typestr="filename"

section "Optional"
option	"skip"		s	"Columns to skip in input PCL"
						int	default="2"
option	"verbosity"	v	"Message verbosity"
						int	default="5"
